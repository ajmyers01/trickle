
<section id="login">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        <div class=div"form-wrap">
          <h1> Login </h1>
          <%= form_tag '/login' do %>
            <p>Email: <%= text_field_tag :email %></p>
            <p>Password: <%= password_field_tag :password %></p>
            <p><%= submit_tag "Submit" %></p>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</section>
